  "ExternalDatabaseInfo": {
    "DataBaseType": "6db454c0-531c-4395-9122-921a18942902",
    "AllowModifySchema": true,
    "ConnectionId": "MySql2",
    "SourceTableName": "v_construction_status_count",
    "SourceTableSchema": "",
    "CreateViewSql": "SELECT\r\n     SUM(subb.estimate_arrange_wait_flg = 1) AS 工事_見積手配待ち件数\r\n    ,SUM(subb.estimate_arrange_pending_flg = 1) AS 工事_見積手配保留件数\r\n    ,SUM(subb.design_wait_flg = 1) AS 工事_設計待ち件数\r\n    ,SUM(subb.design_examination_flg = 1) AS 工事_設計審査中件数\r\n    ,SUM(subb.examination_complete_flg = 1) AS 工事_設計審査完了件数\r\n    ,SUM(subb.order_wait_flg = 1) AS 工事_受注確定待ち件数\r\n    ,SUM(suba.construction_check_flg = 1) AS 工事_工事確認中件数\r\n    ,SUM(suba.construction_check_schedule_flg = 1) AS 工事_日程確認中件数\r\n    ,SUM(suba.construction_report_wait_flg = 1) AS 工事_報告待ち件数\r\n    ,SUM(suba.construction_examination_flg = 1) AS 工事_施工審査中件数\r\n    ,SUM(suba.requesting_correction_flg = 1) AS 工事_審査修正依頼中件数\r\n    ,SUM(suba.report_delivery_wait_flg = 1) AS 工事_報告書納品待ち件数\r\n    ,SUM(suba.construction_complete_wait_flg = 1) AS 工事_完了待ち件数\r\nFROM t_object_construction main\r\nINNER JOIN m_construction_status suba ON main.construction_status_id = suba.construction_status_id\r\nLEFT JOIN m_construction_estimate_status subb ON suba.construction_estimate_status_id = subb.construction_estimate_status_id\r\nWHERE EXISTS(SELECT object_order_id\r\n             FROM t_object_order\r\n             WHERE t_object_order.object_order_id = main.object_order_id\r\n             AND t_object_order.active_flg = 1)\r\nAND(subb.estimate_arrange_wait_flg = 1\r\n OR estimate_arrange_pending_flg = 1\r\n OR subb.design_wait_flg = 1\r\n OR subb.design_examination_flg = 1\r\n OR subb.examination_complete_flg = 1\r\n OR subb.order_wait_flg = 1\r\n OR suba.construction_check_flg = 1\r\n OR suba.construction_check_schedule_flg = 1\r\n OR suba.construction_report_wait_flg = 1\r\n OR suba.construction_examination_flg = 1\r\n OR suba.requesting_correction_flg = 1\r\n OR suba.report_delivery_wait_flg = 1\r\n OR suba.construction_complete_wait_flg = 1)",
    "ViewInfo": {
      "ViewName": "v_construction_status_count",
      "QueryColumns": []
    }
  },
