    "SourceTableSchema": "",
    "CreateViewSql": "SELECT\r\n     SUM(sub.design_wait_flg = 1) AS 転圧_設計待ち件数\r\n    ,SUM(sub.estimate_arrange_wait_flg = 1) AS 転圧_見積手配待ち件数\r\n    ,SUM(sub.estimate_arrange_flg = 1) AS 転圧_見積手配中件数\r\n    ,SUM(sub.wait_order_flg = 1) AS 転圧_受注待ち件数\r\n    ,SUM(sub.compaction_check_schedule_flg = 1) AS 転圧_日程確認中件数\r\n    ,SUM(sub.compaction_confirm_flg = 1) AS 転圧_転圧確認中件数\r\n    ,SUM(sub.wait_enforce_flg = 1) AS 転圧_施工待ち件数\r\n    ,SUM(sub.construction_examination_flg = 1) AS 転圧_施工審査中件数\r\n    ,SUM(sub.resurvey_result_wait_flg = 1) AS 転圧_再調査結果待ち件数\r\n    ,SUM(sub.compaction_complete_wait_flg = 1) AS 転圧_完了待ち件数\r\nFROM t_object_compaction main\r\nINNER JOIN m_compaction_status sub ON main.compaction_status_id = sub.compaction_status_id\r\nWHERE EXISTS(SELECT object_order_id\r\n             FROM t_object_order\r\n             WHERE t_object_order.object_order_id = main.object_order_id\r\n             AND t_object_order.active_flg = 1)\r\nAND(sub.design_wait_flg = 1\r\n OR sub.estimate_arrange_wait_flg = 1\r\n OR sub.estimate_arrange_flg = 1\r\n OR sub.wait_order_flg = 1\r\n OR sub.compaction_check_schedule_flg = 1\r\n OR sub.compaction_confirm_flg = 1\r\n OR sub.wait_enforce_flg = 1\r\n OR sub.construction_examination_flg = 1\r\n OR sub.resurvey_result_wait_flg = 1\r\n OR sub.compaction_complete_wait_flg = 1)",
    "ViewInfo": {
      "ViewName": "v_compaction_status_count",
      "QueryColumns": []
    }
  },
