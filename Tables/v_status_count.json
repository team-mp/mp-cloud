  "ExternalDatabaseInfo": {
    "DataBaseType": "6db454c0-531c-4395-9122-921a18942902",
    "AllowModifySchema": true,
    "ConnectionId": "MySql2",
    "SourceTableName": "v_status_count",
    "SourceTableSchema": "",
    "CreateViewSql": "SELECT\r\nSUM(CASE WHEN c.survey_arrange_wait_flg = 1 THEN 1 ELSE 0 END) AS 調査_手配待ち件数\r\n,SUM(CASE WHEN c.survey_arrange_pending_flg = 1 THEN 1 ELSE 0 END) AS 調査_手配保留件数\r\n,SUM(CASE WHEN c.survey_consent_wait_flg = 1 THEN 1 ELSE 0 END) AS 調査_承諾待ち件数\r\n,SUM(CASE WHEN c.survey_result_wait_flg = 1 THEN 1 ELSE 0 END) AS 調査_結果待ち件数\r\n,SUM(CASE WHEN c.survey_complete_wait_flg = 1 THEN 1 ELSE 0 END) AS 調査_完了待ち件数\r\n,SUM(CASE WHEN e.surveying_arrange_wait_flg = 1 THEN 1 ELSE 0 END) AS 測量_手配待ち件数\r\n,SUM(CASE WHEN e.surveying_arrange_pending_flg = 1 THEN 1 ELSE 0 END) AS 測量_手配保留件数\r\n,SUM(CASE WHEN e.surveying_consent_wait_flg = 1 THEN 1 ELSE 0 END) AS 測量_承諾待ち件数\r\n,SUM(CASE WHEN e.surveying_result_wait_flg = 1 \r\n     AND d.site_survey_flg = 1 THEN 1 ELSE 0 END)\r\n+SUM(CASE WHEN e.surveying_result_wait_flg = 1 \r\n     AND d.gove_survey_flg = 1 THEN 1 ELSE 0 END) AS 測量_結果待ち件数\r\n,SUM(CASE WHEN e.surveying_complete_wait_flg = 1 THEN 1 ELSE 0 END) AS 測量_完了待ち件数\r\n,SUM(CASE WHEN g.analysis_wait_flg = 1 THEN 1 ELSE 0 END) AS 解析_解析待ち件数\r\n,SUM(CASE WHEN g.analysis_pending_flg = 1 THEN 1 ELSE 0 END) AS 解析_解析保留件数\r\n,SUM(CASE WHEN g.office_work_flg = 1 THEN 1 ELSE 0 END) AS 解析_業務対応中件数\r\n,SUM(CASE WHEN g.analysis_complete_wait_flg = 1 THEN 1 ELSE 0 END) AS 解析_完了待ち件数\r\n,SUM(CASE WHEN i.design_wait_flg = 1 THEN 1 ELSE 0 END) AS 転圧_設計待ち件数\r\n,SUM(CASE WHEN i.estimate_arrange_wait_flg = 1 THEN 1 ELSE 0 END) AS 転圧_見積手配待ち件数\r\n,SUM(CASE WHEN i.estimate_arrange_flg = 1 THEN 1 ELSE 0 END) AS 転圧_見積手配中件数\r\n,SUM(CASE WHEN i.wait_order_flg = 1 THEN 1 ELSE 0 END) AS 転圧_受注待ち件数\r\n,SUM(CASE WHEN i.compaction_check_schedule_flg = 1 THEN 1 ELSE 0 END) AS 転圧_日程確認中件数\r\n,SUM(CASE WHEN i.compaction_confirm_flg = 1 THEN 1 ELSE 0 END) AS 転圧_転圧確認中件数\r\n,SUM(CASE WHEN i.plan_complete_wait_flg = 1 THEN 1 ELSE 0 END) AS 転圧_プラン確定待ち件数\r\n,SUM(CASE WHEN i.wait_enforce_flg = 1 THEN 1 ELSE 0 END) AS 転圧_施工待ち件数\r\n,SUM(CASE WHEN i.construction_examination_flg = 1 THEN 1 ELSE 0 END) AS 転圧_施工審査中件数\r\n,SUM(CASE WHEN i.resurvey_result_wait_flg = 1 THEN 1 ELSE 0 END) AS 転圧_再調査結果待ち件数\r\n,SUM(CASE WHEN i.compaction_complete_wait_flg = 1 THEN 1 ELSE 0 END) AS 転圧_完了待ち件数\r\n,SUM(CASE WHEN l.estimate_arrange_wait_flg = 1 THEN 1 ELSE 0 END) AS 工事_見積手配待ち件数\r\n,SUM(CASE WHEN l.design_wait_flg = 1 THEN 1 ELSE 0 END) AS 工事_設計待ち件数\r\n,SUM(CASE WHEN l.design_examination_flg = 1 THEN 1 ELSE 0 END) AS 工事_設計審査中件数\r\n,SUM(CASE WHEN l.examination_complete_flg = 1 THEN 1 ELSE 0 END) AS 工事_設計審査完了件数\r\n,SUM(CASE WHEN l.order_wait_flg = 1 THEN 1 ELSE 0 END) AS 工事_受注確定待ち件数\r\n,SUM(CASE WHEN k.construction_check_flg = 1 THEN 1 ELSE 0 END) AS 工事_工事確認中件数\r\n,SUM(CASE WHEN k.construction_check_schedule_flg = 1 THEN 1 ELSE 0 END) AS 工事_日程確認中件数\r\n,SUM(CASE WHEN k.construction_report_wait_flg = 1 THEN 1 ELSE 0 END) AS 工事_報告待ち件数\r\n,SUM(CASE WHEN k.construction_examination_flg = 1 THEN 1 ELSE 0 END) AS 工事_施工審査中件数\r\n,SUM(CASE WHEN k.report_delivery_wait_flg= 1 THEN 1 ELSE 0 END) AS 工事_報告書納品待ち件数\r\n,SUM(CASE WHEN k.construction_complete_wait_flg= 1 THEN 1 ELSE 0 END) AS 工事_完了待ち件数\r\n,SUM(CASE WHEN n.warranty_issue_wait_flg = 1 THEN 1 ELSE 0 END) AS 保証_保証書発行待ち件数\r\n,SUM(CASE WHEN n.warranty_check_flg = 1 THEN 1 ELSE 0 END) AS 保証_保証確認中件数\r\n,SUM(CASE WHEN n.plan_complete_wait_flg = 1 THEN 1 ELSE 0 END) AS 保証_プラン確定待ち件数\r\n,SUM(CASE WHEN n.warranty_mailing_wait_flg = 1 THEN 1 ELSE 0 END) AS 保証_郵送手配待ち件数\r\n,SUM(CASE WHEN n.mailing_request_wait_flg = 1 THEN 1 ELSE 0 END) AS 保証_郵送依頼待ち件数\r\n\r\nFROM t_object_order a\r\nLEFT JOIN t_object_survey b ON a.latest_survey_id = b.survey_id\r\nLEFT JOIN m_survey_status c ON b.survey_status_id = c.survey_status_id\r\n\r\nLEFT JOIN t_object_surveying d ON a.latest_surveying_id = d.surveying_id\r\nLEFT JOIN m_surveying_status e ON d.surveying_status_id = e.surveying_status_id\r\n\r\nLEFT JOIN t_object_analysis f ON a.latest_analysis_id = f.analysis_id\r\nLEFT JOIN m_analysis_status g ON f.analysis_status_id = g.analysis_status_id\r\n\r\nLEFT JOIN t_object_compaction h ON a.latest_compaction_id = h.compaction_id\r\nLEFT JOIN m_compaction_status i ON h.compaction_status_id = i.compaction_status_id\r\n\r\nLEFT JOIN t_object_construction j ON a.latest_construction_id = j.construction_id\r\nLEFT JOIN m_construction_status k ON j.construction_status_id = k.construction_status_id\r\nLEFT JOIN m_construction_estimate_status l ON k.construction_estimate_status_id = l.construction_estimate_status_id\r\n\r\nLEFT JOIN t_object_warranty m ON a.latest_warranty_id = m.warranty_id\r\nLEFT JOIN m_warranty_status n ON m.warranty_status_id = n.warranty_status_id\r\n\r\nWHERE a.active_flg = 1",
    "ViewInfo": {
      "ViewName": "v_status_count",
      "QueryColumns": []
    }
  },
